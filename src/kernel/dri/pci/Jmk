init(pci, pci.a)

preset(freestanding)
preset(optimize)
preset(debug)
preset(32)
preset(warn)

archetype(c)

CFLAGS += -I$(ROOT)/include/kernel

OBJECTS = pci.o vendors.o

vendors.c: vendors.csv vendors_to_h.awk
	status_log(AWK, vendors.csv)
	@dos2unix < vendors.csv | awk -f vendors_to_h.awk > $@

type(static_lib)

finish
